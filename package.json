{
  "name": "fslightbox-react",
  "version": "1.6.2",
  "description": "React version of Fullscreen Lightbox. Modern and easy plugin for displaying images and videos in clean overlaying box. Display single source or create beautiful gallery with powerful lightbox.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "production": "webpack --mode production --config webpack.prod.config.js --display-modules && gulp",
    "watch": "webpack-dev-server --mode development --host 0.0.0.0",
    "cypress": "concurrently --kill-others \"webpack-dev-server --config ./webpack.cypress.config.js --mode development\" \"node ./node_modules/.bin/cypress open\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/banthagroup/fslightbox-react"
  },
  "jest": {
    "setupFiles": [
      "./jest-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules",
      "/demo/",
      "/dist/",
      "/tests/cypress",
      "/tests/__tests-services__"
    ],
    "collectCoverage": false
  },
  "keywords": [
    "react lightbox",
    "react image lightbox",
    "slide gallery",
    "fslightbox",
    "lightbox",
    "carousel"
  ],
  "author": "Piotr Zdziarski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/banthagroup/fslightbox-react/issues"
  },
  "homepage": "https://fslightbox.com/",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.0.1",
    "css-loader": "^2.1.0",
    "cypress": "^5.6.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "fslightbox-react": "^1.4.5",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "node-sass": "^4.12.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {}
}
